{
  "distSpecVersion": "1.0.1-dev",
  "storage": {
    "rootDirectory": "/tmp/zot"
  },
  "http": {
    "address": "127.0.0.1",
    "port": "8080",
    "realm": "zot",
    "tls": {
      "cert": "test/data/server.cert",
      "key": "test/data/server.key",
      "cacert": "test/data/ca.crt"
    },
    "accessControl": {
      "**": {
        "anonymousPolicy": ["read"],
        "defaultPolicy": ["read"]
      }
    }
  },
  "log": {
    "level": "debug"
  }
}
